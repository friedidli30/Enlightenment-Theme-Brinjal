            align: 0.5 1.0;
         }
         description { state: "over" 0.0;
            inherit: "default" 0.0;
            min: 15 15;
            max: 999999 15;
         }
      }
   }
   programs {
      program { signal: "mouse,down,1*"; source: "e.dragable.vbar";
